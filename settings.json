// Zed settings
//
// For information on how to configure Zed, see the Zed
// documentation: https://zed.dev/docs/configuring-zed
//
// To see all of Zed's default settings without changing your
// custom settings, run the `zed: Open Default Settings` command
// from the command palette
{
  "agent": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-5-mini",
    },
    "favorite_models": [],
    "model_parameters": [],
  },
  "git_panel": {
    "tree_view": false,
  },
  "colorize_brackets": true,
  "icon_theme": "Material Icon Theme",
  "agent_servers": {
    "github-copilot": {
      "type": "registry",
    },
  },
  "edit_predictions": {
    "provider": "copilot",
  },
  "telemetry": {
    "metrics": false,
  },
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "theme": {
    "mode": "dark",
    "light": "One Light",
    "dark": "Darcula based on Ayu Dark",
  },
  "experimental.theme_overrides": {
    "accents": ["#E8BA36", "#1F4F15FF", "#359FF4", "#5060BB", "#179387"],
  },
  "wrap_guides": [80],
}
